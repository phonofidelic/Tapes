{
  "name": "electron_recorder",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.0.0",
    "@material-ui/icons": "^4.0.0",
    "audio-analyser": "^1.0.2",
    "audio-play": "^2.2.1",
    "audio-waveform": "^1.3.0",
    "concurrently": "^4.1.0",
    "connected-react-router": "^6.4.0",
    "cross-env": "^5.2.0",
    "datauri": "^2.0.0",
    "dataurl": "^0.1.0",
    "electron-is-dev": "^1.1.0",
    "gl-waveform": "^4.3.3",
    "history": "^4.9.0",
    "mic": "^2.1.2",
    "naudiodon": "^2.0.3",
    "node-audiorecorder": "^1.4.1",
    "p5": "^0.8.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-p5-wrapper": "^1.2.0",
    "react-redux": "^7.0.3",
    "react-router-dom": "^5.0.0",
    "react-router-redux": "^4.0.8",
    "react-scripts": "3.0.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "speaker": "^0.4.2",
    "styled-components": "^4.2.0",
    "wait-on": "^3.2.0",
    "electron-devtools-installer": "^2.2.4"
  },
  "scripts": {
    "react-start": "react-scripts start",
    "react-build": "react-scripts build",
    "react-test": "react-scripts test",
    "react-eject": "react-scripts eject",
    "electron": "electron ./public/electron.js",
    "rebuild": "./node_modules/.bin/electron-rebuild -f -w speaker",
    "electron-build": "ELECTRON_BUILDER_ALLOW_UNRESOLVED_DEPENDENCIES=true electron-builder",
    "release": "yarn react-build && electron-builder --publish=always",
    "build": "yarn react-build && yarn electron-build",
    "start": "concurrently \"cross-env BROWSER=none yarn react-start\" \"wait-on http://localhost:3000 && electron ./public/electron.js\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron": "^5.0.2",
    "electron-rebuild": "^1.8.5",
    "electron-builder": "^20.43.0"
  },
  "description": "Desktop application for creating and cataloging audio recordings.",
  "author": "phonofidelic",
  "build": {
    "appId": "com.tapes.phonofidelic",
    "files": [
      "**/*",
      "build/**/*",
      "node_modules/**/*"
    ]
  },
  "nodeGypRebuild": true,
  "main": "public/electron.js",
  "homepage": "./"
}
